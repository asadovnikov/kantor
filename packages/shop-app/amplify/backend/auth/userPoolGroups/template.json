{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Parameters": {
        "authshopapp53b8683eUserPoolId": {
            "Type": "String",
            "Default": "authshopapp53b8683eUserPoolId"
        },
        
        "authshopapp53b8683eAppClientID": {
            "Type": "String",
            "Default": "authshopapp53b8683eAppClientID"
        },
        "authshopapp53b8683eAppClientIDWeb": {
            "Type": "String",
            "Default": "authshopapp53b8683eAppClientIDWeb"
        },
        "AuthRoleArn": {
            "Type": "String"
        },
        "UnauthRoleArn": {
            "Type": "String"
        },
        "env": {
            "Type": "String"
        }
    },
    "Conditions": {
        "ShouldNotCreateEnvResources": {
            "Fn::Equals": [
                {
                    "Ref": "env"
                },
                "NONE"
            ]
        }
    },
    "Resources": {
    
        "CustomersGroup": {
          "Type" : "AWS::Cognito::UserPoolGroup",
          "Properties" : {
              "GroupName" : "Customers",
              "Precedence" : 1,
              
              "UserPoolId" : {
                "Ref": "authshopapp53b8683eUserPoolId"
              } 
            }
        },
        
    
        "AdminsGroup": {
          "Type" : "AWS::Cognito::UserPoolGroup",
          "Properties" : {
              "GroupName" : "Admins",
              "Precedence" : 2,
              
              "UserPoolId" : {
                "Ref": "authshopapp53b8683eUserPoolId"
              } 
            }
        },
        
    
    
    },
    
}
