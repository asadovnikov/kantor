type Transaction {
  id: ID!
  firstName: String!
  surname: String!
  dateOfBirth: Date!
  streetLine1: String!
  streetLine2: String
  city: String!
  postalCode: Int!
  stateProvince: String!
  country: String!
  email: String!
  telephone: String
  fiatCurrency: String!
  fiatAmount: Int!
  cryptoCurrency: String
  cryptoAmount: Number
  cryptoAddress: String
  gwReference: String
  bankReference: String
  merchantRef: String
}

type Customer {
  id: ID!
  firstName: String!
  surname: String!
  dateOfBirth: Date!
  streetLine1: String!
  streetLine2: String
  city: String!
  postalCode: Int!
  stateProvince: String!
  country: String!
  email: String!
  telephone: String
  fiatCurrency: String!
  fiatDailyAmount: Int!
  fiatMonthlyAmount: Int!
  tier: String
  KYC_State: String!
}