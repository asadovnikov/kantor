type Transaction {
  id: String
  firstName: String!
  surname: String!
  dateOfBirth: Date!
  streetLine1: String!
  streetLine2: String
  city: String!
  postalCode: Number!
  stateProvince: String!
  country: String!
  email: String!
  telephone: String
  fiatCurrency: String!
  fiatAmount: Number!
  cryptoCurrency: String
  cryptoAmount: Number
  cryptoAddress: String
  gwReference: String
  bankReference: String
  merchantRef: String
}

type Customer {
  id: String
  firstName: String!
  surname: String!
  dateOfBirth: Date!
  streetLine1: String!
  streetLine2: String
  city: String!
  postalCode: Number!
  stateProvince: String!
  country: String!
  email: String!
  telephone: String
  fiatCurrency: String!
  fiatDailyAmount: Number!
  fiatMonthlyAmount: Number!
  tier: String
  KYC_State: String!
}